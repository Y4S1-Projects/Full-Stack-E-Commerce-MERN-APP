{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.5.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "http-proxy-middleware": "^3.0.5",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.4",
  "web-vitals": "^2.1.4",
  "dompurify": "^2.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "react-scripts build",
    "build:secure": "npm run clean && powershell -Command \"$env:GENERATE_SOURCEMAP='false'; react-scripts build\" && npm run scan:ips",
    "build:prod": "npm run clean && powershell -Command \"$env:GENERATE_SOURCEMAP='false'; $env:NODE_ENV='production'; react-scripts build\" && npm run scan:ips",
    "clean": "rimraf build node_modules/.cache",
    "scan:ips": "powershell -ExecutionPolicy Bypass -File \"../scripts/scan-build-for-ips.ps1\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "glob": "^11.0.3",
    "react-app-rewired": "^2.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.14"
  }
}
